<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.firebasetestapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".FirebaseTestApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.In_Thread_Activity"></activity>
        <activity android:name=".Activity.Fragment.MyPage_Fragment" />
        <activity android:name=".Activity.Fragment.ChatRoom_Fragment" />
        <activity android:name=".Activity.Fragment.HomeFragment_Activity" />
        <activity android:name=".Activity.Login_Resister_PassChange.ChangePassword_Activity" />
        <activity android:name=".Activity.Login_Resister_PassChange.Resister_Activity" />
        <activity android:name=".Activity.setting.setting_Activity" />
        <activity android:name=".Activity.setting.profileChange_Activity" />
        <activity android:name=".Activity.ChatLog_Activity" />
        <activity android:name=".Activity.LatestMessage_Activity" />
        <activity android:name=".Activity.SelectUser_Activity" />
        <activity android:name=".Activity.Login_Resister_PassChange.Login_Activity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>